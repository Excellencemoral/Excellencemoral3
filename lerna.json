{
  "version": "2.3.1",
  "command": {
    "version": {
      "allowBranch": [
        "master",
        "release/*"
      ],
      "conventionalCommits": true,
      "message": "chore: Publish %s"
    },
    "publish": {
      "ignoreChanges": [
        "*.md"
      ]
    }
  },
  "packages": [
    "packages/*"
  ]
}
